<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::faq_elements" title="Lof FAQ">
					<resource id="Lof_Faq::question" title="Question" sortOrder="10">
						<resource id="Lof_Faq::question_edit" title="Edit Question" sortOrder="10" />
						<resource id="Lof_Faq::question_save" title="Save Question" sortOrder="20" />
						<resource id="Lof_Faq::question_delete" title="Delete Question" sortOrder="30" />
					</resource>
					<resource id="Lof_Faq::tag" title="Tags" sortOrder="20">
						<resource id="Lof_Faq::tag_edit" title="Edit Tag" sortOrder="10" />
						<resource id="Lof_Faq::tag_save" title="Save Tag" sortOrder="20" />
						<resource id="Lof_Faq::tag_delete" title="Delete Tag" sortOrder="30" />
					</resource>
					<resource id="Lof_Faq::category" title="Category" sortOrder="30">
						<resource id="Lof_Faq::category_edit" title="Edit Category" sortOrder="10" />
						<resource id="Lof_Faq::category_save" title="Save Category" sortOrder="20" />
						<resource id="Lof_Faq::category_delete" title="Delete Category" sortOrder="30" />
					</resource>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Lof_Faq::config_faq" title="Lof FAQ" />
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
